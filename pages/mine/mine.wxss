  border-radius: 30rpx;
  height: 800rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.view-rotaryTable-area {
  position: relative;
  width: 600rpx;
  height: 600rpx;
  border: 2rpx solid #467B73;
  border-radius: 50%;
  overflow: hidden;
  font-size: 30rpx;
}

.view-rotaryTable-line {
  position: absolute;
  width: 600rpx;
  height: 2rpx;
  background: #467B73;
  top: 300rpx;
  left: 0;
}

.view-rotaryTable-content {
  position: absolute;
}

.span-rotaryTable-pointer {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  background-color: #ffcccc;
  border-radius: 50%;
  left: 270rpx;
  top: 270rpx;
}

.span-rotaryTable-pointer::after {
  position: absolute;
  content: ' ';
  width: 0;
  height: 0;
  border-right: 12rpx solid transparent;
  border-left: 12rpx solid transparent;
  border-bottom: 52rpx solid #ffcccc;
  z-index: 10;
  left: 17rpx;
  top: -48rpx;
}