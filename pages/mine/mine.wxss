page {
  background-color: #FBF2E3;
}

.view-top-box {
  position: absolute;
  width: 100%;
  top: 0rpx;
  height: 240rpx;
  background-color: #467B73;
  border-bottom-left-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
  display: flex;
  align-items: center;
}

.img-user-avatar {
  position: relative;
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-left: 35rpx;
  flex-shrink: 0;
}

.img-user-nickName {
  position: relative;
  color: #feffff;
  font-size: 30rpx;
  margin-left: 35rpx;
  flex-shrink: 0;
}

.view-function-box {
  position: relative;
  margin: 270rpx 35rpx 0 35rpx;
  background-color: #FEFFFF;
  border-radius: 30rpx;
}

.view-function {
  position: relative;
  display: flex;
  align-items: center;
  height: 80rpx;
}

.img-function {
  position: relative;
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
  flex-shrink: 0;
}

.text-function {
  position: relative;
  color: #467B73;
  font-size: 30rpx;
  margin-left: 30rpx;
}

.btn-function {
  position: absolute;
  height: 80rpx;
  width: 100% !important;
  background-color: transparent;
}

.img-function-notice {
  position: relative;
  width: 44rpx;
  height: 40rpx;
  margin-left: 30rpx;
  flex-shrink: 0;
}

.view-mask-box {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  background-color: rgba(51, 51, 51, 0.5);
  overflow: hidden;
}

.view-feedback-box {
  position: fixed;
  left: 45rpx;
  right: 45rpx;
  height: 620rpx;
  top: 50%;
  transform: translateY(-50%);
  background-color: #FBF2E3;
  border-radius: 30rpx;
}

.text-viewName {
  position: absolute;
  font-size: 30rpx;
  color: #467B73;
  left: 50%;
  top: 10rpx;
  transform: translateX(-50%);
}

.view-feedback-localPicPathList {
  position: relative;
  margin: 80rpx 35rpx 0 35rpx;
  display: flex;
  align-items: center;
  padding-bottom: 10rpx;
  border-bottom: 1rpx solid #467B73;
}

.view-feedback-picCell {
  position: relative;
  width: 100rpx;
  height: 100rpx;
  flex-shrink: 0;
}

.img-feedback-picCell {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  flex-shrink: 0;
}

.img-feedback-deletePicCell {
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  right: 0rpx;
  top: 0rpx;
  flex-shrink: 0;
}

.view-feedback-addPic {
  position: relative;
  width: 100rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 20rpx;
  color: #467B73;
}

.img-feedback-dottedBox {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  flex-shrink: 0;
}

.img-feedback-picIcon {
  position: relative;
  width: 40rpx;
  height: 40rpx;
  flex-shrink: 0;
}

.view-feedback-content {
  position: relative;
  margin: 10rpx 35rpx 0 35rpx;
  border-bottom: 1rpx solid #467b73;
}

.textarea-feedback-content {
  position: relative;
  font-size: 30rpx;
  width: 100%;
  height: 250rpx;
  color: #467B73;
  padding-bottom: 10rpx;
}

.img-feedback-record {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  right: 50rpx;
  margin-top: 20rpx;
  flex-shrink: 0;
}

.img-feedback-back {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  left: 50rpx;
  margin-top: 20rpx;
  flex-shrink: 0;
}

.view-aboutSelf-box {
  position: fixed;
  left: 45rpx;
  right: 45rpx;
  height: 700rpx;
  top: 50%;
  transform: translateY(-50%);
  background-color: #FBF2E3;
  border-radius: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.text-aboutSelf {
  position: relative;
  font-size: 30rpx;
  color: #467B73;
  margin: 65rpx 35rpx 0 35rpx;
  line-height: 45rpx;
}

.img-praise {
  position: relative;
  width: 100rpx;
  height: 100rpx;
  margin-top: 10rpx;
  flex-shrink: 0;
}

.text-praiseSum {
  position: relative;
  font-size: 30rpx;
  color: #467B73;
}

.view-setNotice-box {
  position: fixed;
  left: 45rpx;
  right: 45rpx;
  height: 280rpx;
  top: 50%;
  transform: translateY(-50%);
  background-color: #FBF2E3;
  border-radius: 30rpx;
}

.view-setNotice-content {
  position: relative;
  margin: 50rpx 35rpx 0 35rpx;
  height: 80rpx;
  border-bottom: 1rpx solid #467B73;
  display: flex;
  align-items: center;
}

.input-setNotice {
  position: relative;
  font-size: 30rpx;
  width: 100%;
  height: 60rpx;
  color: #467B73;
}

.img-setNotice-update {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  right: 50rpx;
  margin-top: 20rpx;
  flex-shrink: 0;
}